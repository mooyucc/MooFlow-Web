{
  "name": "mooplan",
  "productName": "MooPlan",
  "description": "一个智能项目计划平台",
  "author": "XKevin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron electron.cjs",
    "build:electron": "electron-builder"
  },
  "dependencies": {
    "canvg": "^4.0.3",
    "dom-to-image-more": "^3.6.0",
    "fontkit": "^2.0.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.4.0",
    "papaparse": "^5.5.3",
    "pdf-lib": "^1.17.1",
    "react": "^19.1.0",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.1.0",
    "svg2pdf.js": "^2.5.0",
    "tinycolor2": "^1.6.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "vite": "^6.3.5"
  },
  "build": {
    "appId": "com.mooplan.app",
    "productName": "MooPlan",
    "icon": "assets/icon.png",
    "asar": true,
    "files": [
      "dist/**",
      "electron.cjs",
      "preload.js",
      "assets/**/*"
    ],
    "directories": {
      "output": "dmg"
    },
    "mac": {
      "icon": "assets/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  }
}
